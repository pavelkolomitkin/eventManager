<% if (countPages > 1) { %>
<nav aria-label="Page navigation">
    <ul class="pagination">

        <% for (i = 0; i < countPages; i++) { %>
            <% if (i == (page - 1)) { %>
            <li><span class="active"><%- (i + 1) %></span></li>
            <% } else {%>
            <li><a href="#/<%= router.generatePath(routeName, {page: i + 1, date: selectedDate}) %>"><%- (i + 1) %></a></li>
            <% } %>

        <% } %>

    </ul>
</nav>
<% } %>